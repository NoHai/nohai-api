{
    "extends": "tslint:recommended",
    "rulesDirectory": [],
    "rules": {
        "max-line-length": {
            "options": [
                120
            ]
        },
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": {
            "severity": "warning",
            "options": [
                "debug",
                "info",
                "time",
                "timeEnd",
                "trace"
            ]
        },
        "quotemark": [
            true,
            "single"
        ],
        "member-access": [
            true,
            "no-public"
        ],
        "no-irregular-whitespace": true,
        "no-trailing-whitespace": true,
        "whitespace": [
            true,
            "check-branch",
            "check-decl",
            "check-operator",
            "check-module",
            "check-separator",
            "check-rest-spread",
            "check-type",
            "check-typecast",
            "check-type-operator",
            "check-preblock",
            "check-postbrace"
        ]
    },
    "jsRules": {
        "max-line-length": {
            "options": [
                120
            ]
        }
    }
}
